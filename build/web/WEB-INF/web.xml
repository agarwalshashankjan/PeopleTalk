<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Servlets.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DownloadFile</servlet-name>
        <servlet-class>Servlets.DownloadFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadFile</servlet-name>
        <url-pattern>/DownloadFile</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>GetPhoto</servlet-name>
        <servlet-class>Servlets.GetPhoto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetPhoto</servlet-name>
        <url-pattern>/GetPhoto</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>TalkPro</servlet-name>
        <servlet-class>Servlets.TalkPro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TalkPro</servlet-name>
        <url-pattern>/TalkPro</url-pattern>
    </servlet-mapping>
    
</web-app>
